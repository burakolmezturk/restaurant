<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd
        http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-3.8.xsd">
    <changeSet  id="07.12.2020 15:06:01"  author="burak">
        <createTable tableName="cart">
            <column type="int(20)" name="id" autoIncrement="true">
                <constraints primaryKey="true"/>
            </column>
            <column type="int(10)" name="product_id"></column>
            <column type="float" name="total_price"></column>
            <column type="int(10)" name="piece"></column>
            <column type="int(10)" name="place_id"></column>
            <column type="float" name="price"></column>
            <column type="int(10)" name="table_id"></column>
            <column type="int(10)" name="waiter_id"></column>
            <column defaultValueComputed="CURRENT_TIMESTAMP" name="create_date" type="TIMESTAMP"/>
        </createTable>
    </changeSet>


</databaseChangeLog>