<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd
        http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-3.8.xsd">
    <changeSet  id="07.12.2020 15:06:01"  author="burak">
        <createTable tableName="roles">
            <column type="int(20)" name="id" autoIncrement="true">
                <constraints primaryKey="true"/>
            </column>
            <column type="nvarchar(50)" name="name"></column>
        </createTable>
    </changeSet>
    <changeSet  id="07.12.2020 15:06:02"  author="burak">
        <insert  catalogName="restaurant" tableName="roles">
            <column  name="id"  valueNumeric="1"/>
            <column  name="name"  value="ROLE_ADMIN"/>
        </insert>
    </changeSet>


</databaseChangeLog>