<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd
        http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-3.8.xsd">
    <changeSet  id="07.12.2020 15:06:01"  author="burak">
        <createTable tableName="users_role">

            <column name="user_id" type="int">
                <constraints primaryKey="true" nullable="false" foreignKeyName="fk_user_id" references="user(id)"/>
            </column>
            <column name="role_id" type="int">
                <constraints primaryKey="true" nullable="false" foreignKeyName="fk_role_id" references="roles(id)"/>
            </column>
        </createTable>
    </changeSet>
    <changeSet  id="07.12.2020 15:06:02"  author="burak">
        <insert  catalogName="restaurant" tableName="users_role">
            <column  name="user_id"  value="1"/>
            <column  name="role_id"  value="1"/>
        </insert>
    </changeSet>



</databaseChangeLog>