<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd
        http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-3.8.xsd">
    <changeSet  id="08.12.2020 15:06:12"  author="burak">
        <createTable tableName="media">
            <column type="int(20)" name="id" autoIncrement="true">
                <constraints primaryKey="true"/>
            </column>
            <column type="nvarchar(50)" name="file_name"></column>
            <column type="binary" name="file_content"></column>
        </createTable>
    </changeSet>



</databaseChangeLog>